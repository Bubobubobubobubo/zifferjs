{
  "name": "zifferjs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "compile-ziffers-pegjs": "peggy --plugin ./node_modules/ts-pegjs/dist/tspegjs --extra-options-file src/parser/zconfig.json -o src/parser/ziffersParser.ts --cache src/parser/ziffers.pegjs",
    "compile-scala-pegjs": "peggy --plugin ./node_modules/ts-pegjs/dist/tspegjs --extra-options-file src/parser/sconfig.json -o src/parser/scalaParser.ts --cache src/parser/scala.pegjs",
    "test": "vitest --ui",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@vitest/ui": "^0.34.1",
    "peggy": "^3.0.2",
    "ts-pegjs": "^4.2.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.1"
  }
}
